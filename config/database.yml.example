default: &default
  adapter: postgresql
  encoding: unicode
  pool: <%= ENV.fetch("RAILS_MAX_THREADS") { 5 } %>

development:
  <<: *default
  database: notes_dev

test:
  <<: *default
  database: notes_test

production:
  <<: *default
  database: notes_production
  username: notes
  password: <%= ENV['NOTES_DATABASE_PASSWORD'] %>
